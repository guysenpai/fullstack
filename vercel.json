{
  "github": {
    "deploymentEnabled": false
  },
  "builds": [
    {
      "src": "/dist/apps/app/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "/dist/apps/app/*",
      "use": "@vercel/static"
    },
    {
      "src": "/dist/apps/app/browser/*",
      "use": "@vercel/static"
    },
    {
      "src": "/dist/apps/app/server/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/dist/apps/app/server/server.mjs" },
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/dist/apps/app/browser/$1" },
    { "src": "/(.*).(js|css|ico)", "dest": "/dist/apps/app/browser/$1.$2" }
  ]
}
